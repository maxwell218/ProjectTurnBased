/// Macros of default keybindings for mouse and keyboard
#macro KEY_NONE					undefined
#macro KEY_UP					ord("W")
#macro KEY_DOWN					ord("S")
#macro KEY_LEFT					ord("A")
#macro KEY_RIGHT				ord("D")
#macro KEY_SHIFT				vk_shift

#macro KEY_PAUSE				vk_escape
#macro KEY_ACCEPT				vk_enter
#macro KEY_BACK					vk_escape

#macro KEY_SELECT				mb_left
#macro KEY_CANCEL				mb_right